<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/TextArrowBase"
   >

    <TextView
        android:id="@+id/tv_left_toast"
        style="@style/TextBoxBase.LeftLabel"
        android:text="@string/access_number" />

    <ImageView
        android:id="@+id/iv_right_arrow"
        style="@style/SelectorArrow"
        android:contentDescription="@string/title_name_of_activity" />

    <TextView
        android:id="@+id/tv_center_content"
        style="@style/SelectorLeftLabel.SelectorRightLabel"
        android:layout_toLeftOf="@+id/iv_right_arrow"
        android:text="@string/none" />

</RelativeLayout>