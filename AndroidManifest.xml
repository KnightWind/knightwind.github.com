<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sktlab.bizconfmobile"
    android:versionCode="20508"
    android:versionName="2.5.8" >
    
    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="17" />
	<uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
 	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
	<uses-permission android:name="android.permission.ACCESS_MOCK_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- 操作日历数据库，需要添加这些权限 -->
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" />
    <!-- 调用日程提醒，需要添加这个权限 -->
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.SYSTEM_OVERLAY_WINDOW" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <!--  get recent running application, need this permission -->
	<uses-permission android:name="android.permission.GET_TASKS" />
    
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    
    <uses-permission android:name="android.permission.SEND_SMS" ></uses-permission>
    <uses-permission android:name="android.permission.WRITE_SMS" />
    <uses-permission android:name="android.permission.READ_SMS" />
    
	<uses-permission android:name="android.permission.CALL_PHONE" />  
	<uses-permission android:name="android.permission.READ_PHONE_STATE" />  
	<uses-permission android:name="android.permission.PROCESS_OUTGOING_CALLS" /> 
    
    <uses-permission android:name="android.permission.READ_LOGS"></uses-permission> 

	<uses-permission android:name="android.permission.VIBRATE"></uses-permission>
    
	<uses-permission android:name="android.permission.DOWNLOAD_WITHOUT_NOTIFICATION" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" /> 
    
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    
    <application
        android:name=".activity.AppClass"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        
        <activity
            android:name="com.sktlab.bizconfmobile.activity.WelcomeActivity"
            android:theme="@style/Theme.NormalActivity"
            android:screenOrientation="portrait"
            android:launchMode="singleTop"
            android:excludeFromRecents="false"
            android:allowTaskReparenting="true">
            
            <intent-filter>  
                <action android:name="com.sktlab.bizconfmobile.launch.welcomeactivity" />    
                <category android:name="android.intent.category.DEFAULT" />                     
            </intent-filter>
            
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            
            <intent-filter>  
                 
                <action android:name="android.intent.action.VIEW" />    
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE"></category>    
                <!-- 指定启动APP的schema和host,强烈建议使用http等公开的schema,自定义的schema可能无法被邮件等视为超链接 -->
                <data android:scheme="http" android:host="bizconf.mobile.com" />              
            </intent-filter>
        </activity>
        
        <activity
            android:name="com.sktlab.bizconfmobile.activity.MainActivity"
            android:launchMode="singleTask"
            android:theme="@style/Theme.NormalActivity" 
            android:screenOrientation="portrait"
            android:excludeFromRecents="false"
            android:allowTaskReparenting="true">   
                    
           	<intent-filter>
                <action android:name="com.sktlab.bizconf.MainActivity" />    
                <category android:name="android.intent.category.DEFAULT" />                     
            </intent-filter>
            
        </activity>
        
        <activity
            android:name=".activity.AddAccountActivity"
            android:theme="@style/Theme.NormalActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:allowTaskReparenting="true"/>
        
        <activity
            android:name=".activity.VerifyCodeActivity"
            android:theme="@style/Theme.NormalActivity" 
            android:launchMode="singleTop"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.ConferenceActivity"
            android:theme="@style/Theme.NormalActivity" 
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:excludeFromRecents="true"
            android:taskAffinity="com.sktlab.bizconfmobile.conference"
            android:allowTaskReparenting="true"/>    
        <activity
            android:name=".activity.AddParticipantActivity"
            android:theme="@style/Theme.NormalActivity" 
            android:launchMode="singleTop"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.OrderConfActivity"
            android:theme="@style/Theme.NormalActivity" 
            android:launchMode="singleTop"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.AccessNumberActivity"
            android:theme="@style/Theme.NormalActivity" 
            android:launchMode="singleTop"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.RepeatSelectionActivity"
            android:theme="@style/Theme.NormalActivity" 
            android:launchMode="singleTop"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.AccountSettingActivity"
            android:theme="@style/Theme.NormalActivity" 
            android:launchMode="singleTop"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.PasswdProtectActivity"
            android:theme="@style/Theme.NormalActivity" 
            android:launchMode="singleTop"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.VersionStatementActivity"
            android:theme="@style/Theme.NormalActivity" 
            android:launchMode="singleTop"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.AboutUsActivity"
            android:theme="@style/Theme.NormalActivity" 
            android:launchMode="singleTop"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.PhoneAndMail"
            android:theme="@style/Theme.NormalActivity" 
            android:launchMode="singleTop"
            android:screenOrientation="portrait"/>
        
        <receiver android:name=".receiver.NetworkReceiver" >
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
        </receiver>
        
        <receiver android:name=".receiver.PhoneStatReceiver" >
            <intent-filter>
                <action android:name="android.intent.action.PHONE_STATE" />
                <action android:name="android.intent.action.NEW_OUTGOING_CALL" />
            </intent-filter>
        </receiver>
        
        <meta-data android:value="5285927156240b042c09235d" android:name="UMENG_APPKEY"></meta-data>
		<meta-data android:value="hiMarket" android:name="UMENG_CHANNEL"/>
</application>

</manifest>